local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BaconBoss Hub | Wizard Clicker Simulator", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
     
_G.fight = true
_G.train = true

function train()
	 while _G.train == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("Activated"):FireServer()
        wait(0)
	end
end

function fight()
      while _G.fight == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BattleService"):WaitForChild("RE"):WaitForChild("Click"):FireServer()
        wait(0)
    end
end


local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local EggTab = Window:MakeTab({
	Name = "Egg",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp4Tab = Window:MakeTab({
	Name = "Others",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddToggle({
	Name = "Auto Train",
	Default = false,
	Callback = function(Value)
		_G.train = Value
        train()
	end    
})

MainTab:AddLabel("Note: You Can Beat Boss That Stronger Than You x2")

MainTab:AddToggle({
	Name = "Auto Click Fight",
	Default = false,
	Callback = function(Value)
		_G.fight = Value
        fight()
	end    
})

_G.sboss = "Noob"
_G.boss = true
function boss()
      while _G.boss == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BattleService"):WaitForChild("RE"):WaitForChild("Fight"):FireServer(_G.sboss)
        wait(0)
    end
end

MainTab:AddToggle({
	Name = "Auto Fight Boss",
	Default = false,
	Callback = function(Value)
		_G.boss = Value
        boss()
	end    
})

MainTab:AddDropdown({
	Name = "Select",
	Default = "Noob",
	Options = {"Noob", "Wizard", "Ice Wizard", "Cyborg Wizard", "Blue Wizard"},
	Callback = function(Value)
	    _G.sboss = Value  
	end    
})

_G.rb = true

function rb()
     while _G.rb == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("RebirthService"):WaitForChild("RE"):WaitForChild("Rebirth"):FireServer()
     wait(0.5)
    end
end

MainTab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(Value)
		_G.rb = Value
        rb()
	end    
})

 
_G.segg = "Grass"
_G.egg = true
function egg()
      while _G.egg == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RE"):WaitForChild("Purchase"):FireServer(_G.segg, 1)
      wait(0)
    end
end        

EggTab:AddToggle({
	Name = "Auto Egg",
	Default = false,
	Callback = function(Value)
		_G.egg = Value
        egg()
	end    
})

EggTab:AddDropdown({
	Name = "Select",
	Default = "Grass",
	Options = {"Grass", "Bunny"},
	Callback = function(Value)
	    _G.segg = Value  
	end    
})

Tp4Tab:AddButton({
    Name = "Anti AFK",
    Callback = function()
        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        end)
    end
})

Tp4Tab:AddButton({
    Name = "Delete Time Hold Button E",
    Callback = function()
        while true do
            for _, v in next, workspace:GetDescendants() do
                if v:IsA("ProximityPrompt") then
                    v.HoldDuration = 0
                end
            end
            wait(1) 
        end
    end
})

Tp4Tab:AddButton({
    Name = "Server Less People",
    Callback = function()
        local Http = game:GetService("HttpService")
        local TPS = game:GetService("TeleportService")
        local Api = "https://games.roblox.com/v1/games/"

        local _place = game.PlaceId
        local _servers = Api.._place.."/servers/Public?sortOrder=Asc&limit=100"

        local function ListServers(cursor)
            local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
            return Http:JSONDecode(Raw)
        end

        local Server, Next
        repeat
            local Servers = ListServers(Next)
            Server = Servers.data[1]
            Next = Servers.nextPageCursor
        until Server

        TPS:TeleportToPlaceInstance(_place, Server.id, game.Players.LocalPlayer)
    end
})

Tp4Tab:AddButton({
    Name = "Server Hop",
    Callback = function()
        local module = loadstring(game:HttpGet("https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua"))()
        module:Teleport(game.PlaceId)
    end
})

Tp4Tab:AddSlider({
	Name = "Change Speed Power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Tp4Tab:AddSlider({
	Name = "Change Jump Power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})
